<?xml version="1.0" encoding="utf-8"?>
<LinearLayout xmlns:android="http://schemas.android.com/apk/res/android"
    android:layout_width="wrap_content"
    android:layout_height="fill_parent"
    android:orientation="vertical" >

    <LinearLayout
        android:layout_width="fill_parent"
        android:layout_height="25px"
        android:orientation="horizontal" >

        <ImageButton
            android:id="@+id/homebutton1"
            android:layout_width="wrap_content"
            android:layout_height="wrap_content"
            android:layout_marginLeft="12px"
            android:layout_marginTop="11px"
            android:background="@drawable/top_bar_home" />

        <TextView
            android:id="@+id/system_time"
            android:layout_width="wrap_content"
            android:layout_height="wrap_content"
            android:layout_marginLeft="590px"
            android:layout_marginTop="3px"
            android:text="10:10" >
        </TextView>
    </LinearLayout>

    <LinearLayout
        android:layout_width="fill_parent"
        android:layout_height="wrap_content"
        android:background="@drawable/button_background"
        android:orientation="horizontal" >

        <ImageButton
            android:id="@+id/homebutton"
            android:layout_width="wrap_content"
            android:layout_height="wrap_content"
            android:layout_marginLeft="12px"
            android:background="@drawable/button_home" />

        <ImageButton
            android:id="@+id/infobutton"
            android:layout_width="wrap_content"
            android:layout_height="wrap_content"
            android:layout_marginLeft="30dp"
            android:background="@drawable/button_bus_info_pressed" />

        <ImageButton
            android:id="@+id/controlbutton"
            android:layout_width="wrap_content"
            android:layout_height="wrap_content"
            android:layout_marginLeft="30dp"
            android:background="@drawable/button_bus_control_pressed" />

        <ImageButton
            android:id="@+id/cameralbutton"
            android:layout_width="wrap_content"
            android:layout_height="wrap_content"
            android:layout_marginLeft="30dp"
            android:background="@drawable/button_bus_cameral_pressed" />

        <ImageButton
            android:id="@+id/expertbutton"
            android:layout_width="wrap_content"
            android:layout_height="wrap_content"
            android:layout_marginLeft="30dp"
            android:background="@drawable/button_bus_expert_pressed" />
    </LinearLayout>

    <LinearLayout
        android:layout_width="fill_parent"
        android:layout_height="406px"
        android:background="@drawable/infobg"
        android:orientation="vertical" >

        <LinearLayout
            android:layout_width="wrap_content"
            android:layout_height="wrap_content"
            android:orientation="horizontal" >

            <ImageButton
                android:id="@+id/model_btn"
                android:layout_width="wrap_content"
                android:layout_height="wrap_content"
                android:layout_marginLeft="30px"
                android:layout_marginTop="10px"
                android:background="@drawable/eco" >
            </ImageButton>

            <LinearLayout
                android:layout_width="wrap_content"
                android:layout_height="wrap_content"
                android:layout_marginLeft="15px"
                android:layout_marginTop="25px"
                android:orientation="vertical" >

                <ImageView
                    android:layout_width="wrap_content"
                    android:layout_height="wrap_content"
                    android:layout_marginLeft="10px"
                    android:background="@drawable/scoretip" />

                <LinearLayout
                    android:layout_width="wrap_content"
                    android:layout_height="wrap_content"
                    android:layout_marginTop="4px"
                    android:background="@drawable/fakescore"
                    android:orientation="horizontal" >

                    <ImageView
                        android:id="@+id/process_socre"
                        android:layout_width="wrap_content"
                        android:layout_height="wrap_content"
                        android:layout_marginLeft="12px"
                        android:layout_marginTop="6px"
                        android:background="@drawable/socre_0" >
                    </ImageView>

                    <TextView
                        android:id="@+id/socre"
                        android:layout_width="wrap_content"
                        android:layout_height="wrap_content"
                        android:layout_marginLeft="8px"
                        android:layout_marginTop="5px"
                        android:textColor="#333534"
                        android:textStyle="bold" >
                    </TextView>
                </LinearLayout>
            </LinearLayout>
        </LinearLayout>

        <RelativeLayout
            android:layout_width="fill_parent"
            android:layout_height="210px"
            android:orientation="vertical" >

            <ImageView
                android:layout_width="wrap_content"
                android:layout_height="wrap_content"
                android:layout_alignWithParentIfMissing="true"
                android:layout_marginTop="120px"
                android:background="@drawable/busbase" />

            <ImageView
                android:id="@+id/cabinetopen"
                android:layout_width="wrap_content"
                android:layout_height="wrap_content"
                android:layout_alignWithParentIfMissing="true"
                android:layout_marginLeft="58px"
                android:layout_marginTop="70px"
                android:background="@drawable/cabinet_open"
                android:visibility="gone" />

            <ImageView
                android:layout_width="wrap_content"
                android:layout_height="wrap_content"
                android:layout_alignWithParentIfMissing="true"
                android:layout_marginLeft="90px"
                android:background="@drawable/buslighton" />

            <ImageView
                android:id="@+id/buslightoff"
                android:layout_width="wrap_content"
                android:layout_height="wrap_content"
                android:layout_alignWithParentIfMissing="true"
                android:layout_marginLeft="90px"
                android:background="@drawable/buslightoff"
                android:visibility="gone" />

            <ImageView
                android:id="@+id/anim_roadline"
                android:layout_width="wrap_content"
                android:layout_height="wrap_content"
                android:layout_alignWithParentIfMissing="true"
                android:layout_marginLeft="200px"
                android:layout_marginTop="176px" />

            <ImageView
                android:id="@+id/anim_front_wheel"
                android:layout_width="wrap_content"
                android:layout_height="wrap_content"
                android:layout_marginLeft="509px"
                android:layout_marginTop="122px" />

            <ImageView
                android:id="@+id/anim_rear_wheel"
                android:layout_width="wrap_content"
                android:layout_height="wrap_content"
                android:layout_marginLeft="233px"
                android:layout_marginTop="123px" />

            <ImageView
                android:id="@+id/anim_air1"
                android:layout_width="140px"
                android:layout_height="30px"
                android:layout_marginLeft="130px"
                android:layout_marginTop="40px"
                android:src="@drawable/air"
                android:visibility="gone" />

            <ImageView
                android:id="@+id/anim_air2"
                android:layout_width="140px"
                android:layout_height="20px"
                android:layout_marginLeft="130px"
                android:layout_marginTop="65px"
                android:src="@drawable/air"
                android:visibility="gone" />

            <ImageView
                android:id="@+id/anim_air3"
                android:layout_width="140px"
                android:layout_height="20px"
                android:layout_marginLeft="130px"
                android:layout_marginTop="80px"
                android:src="@drawable/air"
                android:visibility="gone" />

            <ImageView
                android:id="@+id/anim_air4"
                android:layout_width="140px"
                android:layout_height="20px"
                android:layout_marginLeft="420px"
                android:layout_marginTop="50px"
                android:src="@drawable/air"
                android:visibility="gone" />

            <ImageView
                android:id="@+id/anim_air5"
                android:layout_width="140px"
                android:layout_height="20px"
                android:layout_marginLeft="420px"
                android:layout_marginTop="65px"
                android:src="@drawable/air"
                android:visibility="gone" />

            <ImageView
                android:id="@+id/anim_air6"
                android:layout_width="140px"
                android:layout_height="20px"
                android:layout_marginLeft="420px"
                android:layout_marginTop="80px"
                android:src="@drawable/air"
                android:visibility="gone" />

            <ImageView
                android:id="@+id/midopenlighton"
                android:layout_width="wrap_content"
                android:layout_height="wrap_content"
                android:layout_marginLeft="300px"
                android:layout_marginTop="50px"
                android:src="@drawable/mid_open_lighton"
                android:visibility="gone" />

            <ImageView
                android:id="@+id/midopenlightoff"
                android:layout_width="wrap_content"
                android:layout_height="wrap_content"
                android:layout_marginLeft="300px"
                android:layout_marginTop="50px"
                android:src="@drawable/mid_open_lightoff"
                android:visibility="gone" />

            <ImageView
                android:id="@+id/frontopenlighton"
                android:layout_width="wrap_content"
                android:layout_height="wrap_content"
                android:layout_marginLeft="570px"
                android:layout_marginTop="55px"
                android:src="@drawable/front_open_lighton"
                android:visibility="gone" />

            <ImageView
                android:id="@+id/frontopenlightoff"
                android:layout_width="wrap_content"
                android:layout_height="wrap_content"
                android:layout_marginLeft="570px"
                android:layout_marginTop="55px"
                android:src="@drawable/front_open_lightoff"
                android:visibility="gone" />

            <ImageView
                android:id="@+id/frontlighton"
                android:layout_width="wrap_content"
                android:layout_height="wrap_content"
                android:layout_marginLeft="640px"
                android:layout_marginTop="75px"
                android:src="@drawable/front_light_on"
                android:visibility="gone" />

            <ImageView
                android:id="@+id/frontheadlighton"
                android:layout_width="wrap_content"
                android:layout_height="wrap_content"
                android:layout_marginLeft="640px"
                android:layout_marginTop="75px"
                android:src="@drawable/front_head_light_on"
                android:visibility="gone" />

            <ImageView
                android:id="@+id/rearlighton"
                android:layout_width="wrap_content"
                android:layout_height="wrap_content"
                android:layout_marginLeft="95px"
                android:layout_marginTop="100px"
                android:src="@drawable/rear_light_on"
                android:visibility="gone" />
        </RelativeLayout>

        <LinearLayout
            android:layout_width="fill_parent"
            android:layout_height="wrap_content"
            android:orientation="horizontal" >

            <ImageView
                android:id="@+id/status_power_panel_battery"
                android:layout_width="wrap_content"
                android:layout_height="wrap_content"
                android:layout_marginLeft="30dp"
                android:layout_marginTop="20dp"
                android:src="@drawable/battery" />

            <LinearLayout
                android:layout_width="130dp"
                android:layout_height="wrap_content"
                android:layout_marginLeft="10dp"
                android:orientation="vertical" >

                <TextView
                    android:id="@+id/status_battery_panel_number"
                    android:layout_width="wrap_content"
                    android:layout_height="wrap_content"
                    android:layout_gravity="center"
                    android:layout_marginLeft="0px"
                    android:textColor="#333534"
                    android:textSize="40px" />

                <ImageView
                    android:id="@+id/power_progressbar"
                    android:layout_width="wrap_content"
                    android:layout_height="wrap_content"
                    android:layout_marginLeft="5px"
                    android:src="@drawable/process_bg0" />

                <ImageView
                    android:layout_width="wrap_content"
                    android:layout_height="wrap_content"
                    android:background="@drawable/soc" />
            </LinearLayout>

            <ImageView
                android:layout_width="wrap_content"
                android:layout_height="fill_parent"
                android:layout_marginLeft="15dp"
                android:layout_marginTop="5dp"
                android:src="@drawable/line" />

            <LinearLayout
                android:layout_width="110dp"
                android:layout_height="fill_parent"
                android:layout_marginLeft="20dp"
                android:orientation="vertical" >

                <LinearLayout
                    android:layout_width="wrap_content"
                    android:layout_height="wrap_content"
                    android:orientation="horizontal" >

                    <TextView
                        android:id="@+id/status_energy_panel_number"
                        android:layout_width="wrap_content"
                        android:layout_height="wrap_content"
                        android:layout_marginLeft="10px"
                        android:textColor="#333534"
                        android:textSize="40px" />

                    <ImageView
                        android:layout_width="wrap_content"
                        android:layout_height="wrap_content"
                        android:layout_marginLeft="5px"
                        android:layout_marginTop="15px"
                        android:background="@drawable/kwh" />
                </LinearLayout>

                <ImageView
                    android:id="@+id/energy_progressbar"
                    android:layout_width="wrap_content"
                    android:layout_height="wrap_content"
                    android:layout_marginLeft="5px"
                    android:src="@drawable/process_bg0" />

                <ImageView
                    android:layout_width="wrap_content"
                    android:layout_height="wrap_content"
                    android:background="@drawable/energy" />
            </LinearLayout>

            <ImageView
                android:layout_width="wrap_content"
                android:layout_height="fill_parent"
                android:layout_centerHorizontal="true"
                android:layout_marginLeft="15dp"
                android:layout_marginTop="5dp"
                android:src="@drawable/line" />

            <LinearLayout
                android:layout_width="110dp"
                android:layout_height="fill_parent"
                android:layout_marginLeft="20dp"
                android:orientation="vertical" >

                <LinearLayout
                    android:layout_width="wrap_content"
                    android:layout_height="wrap_content"
                    android:orientation="horizontal" >

                    <TextView
                        android:id="@+id/status_distance_panel_number"
                        android:layout_width="wrap_content"
                        android:layout_height="wrap_content"
                        android:layout_marginLeft="10px"
                        android:textColor="#333534"
                        android:textSize="40px" />

                    <ImageView
                        android:layout_width="wrap_content"
                        android:layout_height="wrap_content"
                        android:layout_marginLeft="5px"
                        android:layout_marginTop="15px"
                        android:background="@drawable/km" />
                </LinearLayout>

                <ImageView
                    android:id="@+id/mile_progressbar"
                    android:layout_width="wrap_content"
                    android:layout_height="wrap_content"
                    android:layout_marginLeft="5px"
                    android:src="@drawable/process_bg0" />

                <ImageView
                    android:layout_width="wrap_content"
                    android:layout_height="wrap_content"
                    android:layout_centerHorizontal="true"
                    android:background="@drawable/mileage" />
            </LinearLayout>

            <ImageView
                android:layout_width="wrap_content"
                android:layout_height="fill_parent"
                android:layout_centerHorizontal="true"
                android:layout_marginLeft="15dp"
                android:layout_marginTop="5dp"
                android:src="@drawable/line" />

            <LinearLayout
                android:layout_width="200dp"
                android:layout_height="fill_parent"
                android:layout_marginLeft="20dp"
                android:orientation="vertical" >

                <com.mark.bus.app.MyView
                    android:id="@+id/myView"
                    android:layout_width="wrap_content"
                    android:layout_height="60dp" />

                <ImageView
                    android:layout_width="wrap_content"
                    android:layout_height="wrap_content"
                    android:layout_centerHorizontal="true"
                    android:src="@drawable/energyconsumption" />
            </LinearLayout>
        </LinearLayout>
    </LinearLayout>

    <LinearLayout
        android:id="@+id/fragment_container"
        android:layout_width="fill_parent"
        android:layout_height="350px"
        android:background="@drawable/bg3"
        android:orientation="vertical" >
    </LinearLayout>

    <LinearLayout
        android:id="@+id/air_bg"
        android:layout_width="fill_parent"
        android:layout_height="wrap_content"
        android:background="@drawable/airbg"
        android:orientation="horizontal" >

        <ImageButton
            android:id="@+id/item_list_home"
            android:layout_width="wrap_content"
            android:layout_height="wrap_content"
            android:layout_marginLeft="10px"
            android:layout_marginTop="20px"
            android:background="@drawable/home" />

        <ImageButton
            android:id="@+id/ac_shut_down"
            android:layout_width="wrap_content"
            android:layout_height="wrap_content"
            android:layout_marginLeft="10px"
            android:layout_marginTop="20px"
            android:background="@drawable/air_off" />

        <LinearLayout
            android:layout_width="90px"
            android:layout_height="fill_parent"
            android:layout_marginLeft="20px"
            android:orientation="vertical" >

            <ImageButton
                android:id="@+id/ac_uparrow"
                android:layout_width="wrap_content"
                android:layout_height="wrap_content"
                android:layout_gravity="center"
                android:layout_marginTop="3px"
                android:background="@drawable/uparrow"
                android:gravity="center" />

            <TextView
                android:id="@+id/ac_text"
                android:layout_width="82px"
                android:layout_height="32px"
                android:layout_gravity="center"
                android:layout_marginTop="3px"
                android:background="@drawable/numberbg"
                android:gravity="center"
                android:textColor="#99b0c5"
                android:textSize="20px" />

            <ImageButton
                android:id="@+id/ac_downarrow"
                android:layout_width="wrap_content"
                android:layout_height="wrap_content"
                android:layout_gravity="center"
                android:layout_marginTop="3px"
                android:background="@drawable/downarrow"
                android:gravity="center" />
        </LinearLayout>

        <TextView
            android:id="@+id/item_list_temprature_text"
            android:layout_width="wrap_content"
            android:layout_height="wrap_content"
            android:layout_marginLeft="3px"
            android:layout_marginTop="45px"
            android:text="@string/kongtiao"
            android:textColor="#9aa7b4"
            android:textSize="14px"
            android:textStyle="bold" />

        <!-- 横杆 温度调节 -->

        <LinearLayout
            android:layout_width="290px"
            android:layout_height="fill_parent"
            android:orientation="vertical" >

            <LinearLayout
                android:layout_width="wrap_content"
                android:layout_height="wrap_content"
                android:orientation="horizontal" >

                <ImageView
                    android:id="@+id/ac_modle_image"
                    android:layout_width="wrap_content"
                    android:layout_height="wrap_content"
                    android:layout_marginTop="20px"
                    android:layout_marginLeft="30px"
                    android:background="@drawable/acsaofeng" >
                </ImageView>

                <ImageButton
                    android:id="@+id/ac_model"
                    android:layout_width="200px"
                    android:layout_height="60px"
                    android:background="@drawable/ac_model" >
                </ImageButton>
            </LinearLayout>

            <LinearLayout
                android:layout_width="wrap_content"
                android:layout_height="wrap_content"
                android:layout_marginTop="30px"
                android:orientation="horizontal" >

                <TextView
                    android:id="@+id/real_temperature"
                    android:layout_width="wrap_content"
                    android:layout_height="wrap_content"
                    android:layout_marginLeft="25px"
                    android:text="22℃"
                    android:textSize="26px" />

                <TextView
                    android:id="@+id/acfengsudangwei"
                    android:layout_width="wrap_content"
                    android:layout_height="wrap_content"
                    android:layout_marginLeft="10px"
                    android:textColor="#ff9900"
                    android:textSize="26px" />

                <SeekBar
                    android:id="@+id/item_list_temprature_seekbar"
                    android:layout_width="140px"
                    android:layout_height="30px"
                    android:layout_marginLeft="11px"
                    android:layout_marginTop="3px"
                    android:max="100"
                    android:maxHeight="6px"
                    android:paddingLeft="-15dip"
                    android:paddingRight="-15dip"
                    android:progress="3"
                    android:progressDrawable="@drawable/fun_score"
                    android:thumb="@drawable/fun_seekbar_thumb"
                    android:thumbOffset="0dip" />

                <!-- 横杆 温度调节 完 -->

            </LinearLayout>

            <!-- 功率 -->
        </LinearLayout>

        <TextView
            android:id="@+id/item_list_power_text"
            android:layout_width="wrap_content"
            android:layout_height="wrap_content"
            android:layout_marginLeft="11px"
            android:layout_marginTop="45px"
            android:text="@string/gonglv"
            android:textColor="#9aa7b4"
            android:textSize="14px"
            android:textStyle="bold" />

        <!-- 功率 完 -->

        <LinearLayout
            android:layout_width="wrap_content"
            android:layout_height="fill_parent"
            android:orientation="vertical" >

            <ImageButton
                android:id="@+id/power_uparrow"
                android:layout_width="wrap_content"
                android:layout_height="wrap_content"
                android:layout_gravity="center"
                android:layout_marginTop="3px"
                android:background="@drawable/uparrow"
                android:gravity="center" />

            <TextView
                android:id="@+id/power_text"
                android:layout_width="82px"
                android:layout_height="32px"
                android:layout_gravity="center"
                android:background="@drawable/numberbg"
                android:gravity="center"
                android:textColor="#99b0c5"
                android:textSize="20px" />

            <ImageButton
                android:id="@+id/power_downarrow"
                android:layout_width="wrap_content"
                android:layout_height="wrap_content"
                android:layout_gravity="center"
                android:layout_marginTop="3px"
                android:background="@drawable/downarrow"
                android:gravity="center" />
        </LinearLayout>
    </LinearLayout>

</LinearLayout>